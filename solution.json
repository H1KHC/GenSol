[
	{
		"type":"config",
		"includeDir":[
			"include"
		],
		"distDir":"bin/",
		"srcDir":"src"
	}, {
		"type":"compiler",
		"executableName":"g++",
		"compileFlag":[
			"-std=c++11"
		]
	}, {
		"type":"compiler",
		"name":"debug",
		"compileFlag":[
			"-g",
			"-D_DEBUG"
		]
	}, {
		"type":"compiler",
		"name":"release",
		"compileFlag":[
			"-O2"
		]
	}, {
		"type":"linker",
		"executableName":"g++",
		"linkFlag":[
			"-std=c++11",
			"-lboost_program_options-mt",
			"-lboost_system-mt",
			"-lboost_filesystem-mt",
			"-static-libgcc"
		],
		"outputFlag":"-o"
	}, {
		"type":"linker",
		"name":"debug",
		"compileFlag":"-g"
	}, {
		"type":"linker",
		"name":"release",
		"compileFlag":"-O2"
	}, {
		"type":"target",
		"name":"gensol",
		"srcR": ".*\\.cpp",
		"compiler":"release",
		"linker":"release"
	}, {
		"type":"target",
		"name":"gensold",
		"srcR": ".*\\.cpp",
		"compiler":"debug",
		"linker":"debug"
	}, {
		"type":"task",
		"default":true,
		"name":"release",
		"target":"gensol"
	}, {
		"type":"task",
		"name":"debug",
		"target":"gensold"
	}
]